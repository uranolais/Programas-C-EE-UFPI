Algoritmo "Exercicio 2 lista 3"
// Autor(a)    : Jardeanne da Silva Ferreira

Var
numero,soma,media: real;
I,qtd : inteiro;


Inicio
soma := 0
qtd := 0

para I:=1 ate 10 faca
escreval ("Entre com um numero")
leia (numero)

qtd := qtd+1
soma := soma+numero
fimpara

media := soma/qtd

escreval("Media dos numeros digitados = ", media)



Fimalgoritmo



